<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.ClientSignUp">
   <children>
      <ImageView fitHeight="401.0" fitWidth="600.0" opacity="0.47" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../purple-or-violet-abstract-watercolor-background-vector.jpg" />
         </image>
      </ImageView>
      <Label layoutX="143.0" layoutY="14.0" text="Please enter your information carefully">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtId" layoutX="69.0" layoutY="83.0" prefHeight="25.0" prefWidth="167.0" promptText="ID" />
      <Text layoutX="-138.0" layoutY="5.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      <TextField fx:id="txtName" layoutX="69.0" layoutY="136.0" prefHeight="25.0" prefWidth="167.0" promptText="Name" />
      <TextField fx:id="txtlastName" layoutX="350.0" layoutY="136.0" promptText="LastName" />
      <TextField fx:id="txtEmail" layoutX="350.0" layoutY="188.0" prefHeight="25.0" prefWidth="149.0" promptText="Email" />
