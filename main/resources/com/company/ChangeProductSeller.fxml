<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.ChangeProductSeller">
   <children>
      <ListView fx:id="productListview" layoutX="84.0" layoutY="45.0" onMouseClicked="#pressonProduct" prefHeight="200.0" prefWidth="433.0" />
      <RadioButton fx:id="nameRadio" layoutX="141.0" layoutY="266.0" mnemonicParsing="false" onAction="#pressonName" text="name" />
      <RadioButton fx:id="priceRadio" layoutX="357.0" layoutY="266.0" mnemonicParsing="false" onAction="#pressonPrice" text="price" />
      <TextField fx:id="newText" layoutX="226.0" layoutY="304.0" />
      <Button fx:id="btnDone" layoutX="274.0" layoutY="341.0" mnemonicParsing="false" onAction="#pressonDone" text="done" />
   </children>
</AnchorPane>
