<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #C39BD3;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.SellerPanel">
   <children>
      <ImageView fitHeight="401.0" fitWidth="608.0" layoutY="70.0" opacity="0.35" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../download.jpg" />
         </image>
      </ImageView>
      <Label layoutX="202.0" layoutY="35.0" text="Welcome to Seller Panel">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnChangepersonalinfo" layoutX="106.0" layoutY="114.0" mnemonicParsing="false" onAction="#pressonChangepersonalinfo" text="Change personal info">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnEditProductInfo" layoutX="381.0" layoutY="114.0" mnemonicParsing="false" onAction="#pressonbtnEditProductInfo" prefHeight="25.0" prefWidth="137.0" text="Edit a product info">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnDeleteaproduct" layoutX="106.0" layoutY="173.0" mnemonicParsing="false" onAction="#pressonbtnDeleteaproduct" prefHeight="25.0" prefWidth="137.0" text="Delete a product">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnAddaProduct" layoutX="381.0" layoutY="173.0" mnemonicParsing="false" onAction="#pressonAddaProduct" prefHeight="25.0" prefWidth="137.0" text="Add a product">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnSaleHistory" layoutX="105.0" layoutY="226.0" mnemonicParsing="false" onAction="#pressonSaleHistory" text="view your sale history">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnLogout" layoutX="419.0" layoutY="226.0" mnemonicParsing="false" onAction="#pressonLogout" text="Log Out">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
